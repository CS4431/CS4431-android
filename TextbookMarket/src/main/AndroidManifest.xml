<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lakehead.textbookmarket" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.lakehead.textbookmarket.MainActivity"
            android:label="@string/app_name" >

        </activity>
        <activity
            android:name="com.lakehead.textbookmarket.SettingsActivity"
            android:label="@string/title_activity_settings"
            android:parentActivityName="com.lakehead.textbookmarket.MainActivity" >
        </activity>
        <activity
            android:name="com.lakehead.textbookmarket.Scanner"
            android:label="@string/title_activity_scanner"
            android:parentActivityName="com.lakehead.textbookmarket.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>
